<?xml version="1.0"?>
<robot name="RFFootSphere">

  <link name="base_link"/>
  <joint name="base_link_J" type="fixed">
      <parent link="base_link"/>
      <child link="Thorax"/>
      <origin rpy="0 0 1.570796" xyz="0.001 -0.0382 -0.1982"/>
   </joint>

  <link name="Thorax"/>

   <joint name="thorax_rffoot_rom_joint" type="fixed">
      <parent link="Thorax"/>
      <child link="thorax_rffoot_rom"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
   </joint>

   <link name="thorax_rffoot_rom">
      <visual>
         <origin xyz="0 0 0" rpy="0 0 0"/>
         <geometry>
            <mesh filename="package://hpp-rbprm-corba/meshes/ant/rffoot_rom.stl"/>
         </geometry>
      </visual>
      <collision>
         <origin xyz="0 0 0" rpy="0 0 0"/>
         <geometry>
            <mesh filename="package://hpp-rbprm-corba/meshes/ant/rffoot_rom.stl"/>
         </geometry>
      </collision>
   </link>
   
   <link name="Abdomen_rx"/>
   <link name="Abdomen_ry"/>
   <link name="Abdomen"/>
   <link name="Head_rx"/>
   <link name="Head_ry"/>
   <link name="Head"/>

   <link name="LAntena"/>
   <link name="LAntenaFoot"/>
   <link name="RAntena"/>
   <link name="RAntenaFoot"/>

  <!-- JOINTS -->
  <joint name="Abdomen_rx" type="revolute">
      <parent link="Thorax"/>
      <child link="Abdomen_rx"/>
      <origin rpy="0 0 0" xyz="0 0.15853 0.19806"/>
      <axis xyz="1 0 0"/>
      <limit effort="0" lower="-0.2" upper="0.2" velocity="1.0"/>
   </joint>
   <joint name="Abdomen_ry" type="revolute">
      <parent link="Abdomen_rx"/>
      <child link="Abdomen_ry"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <axis xyz="0 1 0"/>
      <limit effort="0" lower="-0.2" upper="0.2" velocity="1.0"/>
   </joint>
   <joint name="Abdomen_rz" type="revolute">
      <parent link="Abdomen_rx"/>
      <child link="Abdomen"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <axis xyz="0 0 1"/>
      <limit effort="0" lower="-0.2" upper="0.2" velocity="1.0"/>
   </joint>
   <joint name="Head_rx" type="revolute">
      <parent link="Thorax"/>
      <child link="Head_rx"/>
      <origin rpy="0 0 0" xyz="0 -0.06471 0.21279"/>
      <axis xyz="1 0 0"/>
      <limit effort="0" lower="-0.2" upper="0.2" velocity="1.0"/>
   </joint>
   <joint name="Head_ry" type="revolute">
      <parent link="Head_rx"/>
      <child link="Head_ry"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <axis xyz="0 1 0"/>
      <limit effort="0" lower="-0.2" upper="0.2" velocity="1.0"/>
   </joint>
   <joint name="Head_rz" type="revolute">
      <parent link="Head_ry"/>
      <child link="Head"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <axis xyz="0 0 1"/>
      <limit effort="0" lower="-0.2" upper="0.2" velocity="1.0"/>
   </joint>
   
   <joint name="LAntena" type="fixed">
      <parent link="Head"/>
      <child link="LAntena"/>
      <origin rpy="0 0 0" xyz="0.0176 -0.1027 0.0225"/>
   </joint>
   <joint name="LAntenaFoot" type="fixed">
      <parent link="LAntena"/>
      <child link="LAntenaFoot"/>
      <origin rpy="0 0 0" xyz="0.0267 -0.0198 0.1006"/>
   </joint>
   
   <joint name="RAntena" type="fixed">
      <parent link="Head"/>
      <child link="RAntena"/>
      <origin rpy="0 0 0" xyz="-0.0168 -0.1029 0.0226"/>
   </joint>
   <joint name="RAntenaFoot" type="fixed">
      <parent link="RAntena"/>
      <child link="RAntenaFoot"/>
      <origin rpy="0 0 0" xyz="-0.0279 -0.0202 0.1006"/>
   </joint>
</robot>
