<?xml version="1.0"?>
<robot name="FrogLHandSphere">

  <link name="base_link"/>
  <joint name="base_link_J" type="fixed">
      <parent link="base_link"/>
      <child link="Torso"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
   </joint>

   <link name="Torso"/>
   <link name="TorsoHead_J1"/>
   <link name="TorsoHead_J2"/>
   <link name="Head"/>

  <joint name="head_lhand_rom_joint" type="fixed">
      <parent link="Head"/>
      <child link="head_lhand_rom"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
   </joint>

  <link name="head_lhand_rom">
      <visual>
         <origin xyz="0 0 0" rpy="0 0 0"/>
         <geometry>
            <mesh filename="package://hpp-rbprm-corba/meshes/frog/lhand_rom.stl"/>
         </geometry>
      </visual>
   </link>


  <!-- JOINTS -->
  <joint name="TorsoHead_J1" type="revolute">
      <parent link="Torso"/>
      <child link="TorsoHead_J1"/>
      <origin rpy="0 0 0" xyz="0.0296 0.0000897 0.0205"/>
      <axis xyz="1 0 0"/>
      <limit effort="0" lower="-0.3" upper="0.3" velocity="1.0"/>
   </joint>
   <joint name="TorsoHead_J2" type="revolute">
      <parent link="TorsoHead_J1"/>
      <child link="TorsoHead_J2"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <axis xyz="0 1 0"/>
      <limit effort="0" lower="-0.3" upper="0.3" velocity="1.0"/>
   </joint>
   <joint name="Head" type="revolute">
      <parent link="TorsoHead_J2"/>
      <child link="Head"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <axis xyz="0 0 1"/>
      <limit effort="0" lower="-0.3" upper="0.3" velocity="1.0"/>
   </joint>
</robot>

