<?xml version="1.0"?>
<robot name="FrogRHandSphere">

  <link name="base_link"/>
  <joint name="base_link_J" type="fixed">
      <parent link="base_link"/>
      <child link="Torso"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
   </joint>

   <link name="Torso"/>
   <link name="Head_rx"/>
   <link name="Head_ry"/>
   <link name="Head"/>

  <joint name="head_rhand_rom_joint" type="fixed">
      <parent link="Head"/>
      <child link="head_rhand_rom"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
   </joint>

  <link name="head_rhand_rom">
      <visual>
         <origin xyz="0 0 0" rpy="0 0 0"/>
         <geometry>
            <mesh filename="package://hpp-rbprm-corba/meshes/frog/rhand_rom.stl"/>
         </geometry>
      </visual>
      <collision>
		 <origin xyz="0 0 0" rpy="0 0 0"/>
         <geometry>
            <mesh filename="package://hpp-rbprm-corba/meshes/frog/rhand_rom.stl"/>
         </geometry>
	  </collision>
   </link>
   

  <!-- JOINTS -->
  <joint name="Head_rx" type="revolute">
      <parent link="Torso"/>
      <child link="Head_rx"/>
      <origin rpy="0 0 0" xyz="0.0296 0.0000897 0.0205"/>
      <axis xyz="1 0 0"/>
      <limit effort="0" lower="-0.3" upper="0.3" velocity="1.0"/>
   </joint>
   <joint name="Head_ry" type="revolute">
      <parent link="Head_rx"/>
      <child link="Head_ry"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <axis xyz="0 1 0"/>
      <limit effort="0" lower="-0.3" upper="0.3" velocity="1.0"/>
   </joint>
   <joint name="Head_rz" type="revolute">
      <parent link="Head_ry"/>
      <child link="Head"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <axis xyz="0 0 1"/>
      <limit effort="0" lower="-0.3" upper="0.3" velocity="1.0"/>
   </joint>
</robot>

