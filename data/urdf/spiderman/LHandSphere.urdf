<?xml version="1.0"?>
<robot name="LHandSphere">

  <link name="base_link"/>
  <joint name="base_link_J" type="fixed">
      <parent link="base_link"/>
      <child link="Pelvis"/>
      <origin rpy="0 0 1.57079632" xyz="0 0 0"/>
   </joint>

   <link name="Pelvis"/>
   <link name="PelvisThorax_J1"/>
   <link name="PelvisThorax_J2"/>
   <link name="Thorax"/>

  <joint name="thorax_lhand_rom_joint" type="fixed">
      <parent link="Thorax"/>
      <child link="thorax_lhand_rom"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
   </joint>

  <link name="thorax_lhand_rom">
      <visual>
         <origin xyz="0 0 0" rpy="0 0 0"/>
         <geometry>
            <mesh filename="package://hpp-rbprm-corba/meshes/spiderman/lhand_rom.stl"/>
         </geometry>
      </visual>
	  <collision>
         <origin xyz="0 0 0" rpy="0 0 0"/>
         <geometry>
            <mesh filename="package://hpp-rbprm-corba/meshes/spiderman/lhand_rom.stl"/>
         </geometry>
      </collision>
   </link>


   <link name="ThoraxSkull_J1"/>
   <link name="ThoraxSkull_J2"/>
   <link name="Skull"/>

 <joint name="A_PelvisThorax_J1" type="revolute"> <!-- A_ added for order while parsing with ROS -->
      <parent link="Pelvis"/>
      <child link="PelvisThorax_J1"/>
      <origin rpy="0 0 0" xyz="0.0006 -0.0373 0.0788"/>
      <axis xyz="1 0 0"/>
      <limit effort="0" lower="-1" upper="1" velocity="1.0"/>
   </joint>
   <joint name="PelvisThorax_J2" type="revolute">
      <parent link="PelvisThorax_J1"/>
      <child link="PelvisThorax_J2"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <axis xyz="0 0 1"/>
      <limit effort="0" lower="-1.5" upper="1.5" velocity="1.0"/>
   </joint>
   <joint name="Thorax" type="revolute">
      <parent link="PelvisThorax_J2"/>
      <child link="Thorax"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <axis xyz="0 1 0"/>
      <limit effort="0" lower="-0.8" upper="0.8" velocity="1.0"/>
   </joint>
   <joint name="A_ThoraxSkull_J1" type="revolute"> <!-- A_ added for order while parsing with ROS -->
      <parent link="Thorax"/>
      <child link="ThoraxSkull_J1"/>
      <origin rpy="0 0 0" xyz="-0.0006 0.0160 0.5204"/>
      <axis xyz="1 0 0"/>
      <limit effort="0" lower="-0.3" upper="0.3" velocity="1.0"/>
   </joint>
   <joint name="ThoraxSkull_J2" type="revolute">
      <parent link="ThoraxSkull_J1"/>
      <child link="ThoraxSkull_J2"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <axis xyz="0 0 1"/>
      <limit effort="0" lower="-1.4" upper="1.4" velocity="1.0"/>
   </joint>
   <joint name="Skull" type="revolute">
      <parent link="ThoraxSkull_J2"/>
      <child link="Skull"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <axis xyz="0 1 0"/>
      <limit effort="0" lower="-1.1" upper="0.4" velocity="1.0"/>
   </joint>
</robot>
